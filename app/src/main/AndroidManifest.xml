<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.campus.management">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_new_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/app_new_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Splash1"
            android:label="@string/title_activity_splash1"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".UpdateStruct"></activity>
        <activity android:name=".FeeStatus" />
        <activity
            android:name=".FeeStructure"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LogIn"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ViewFee"
            android:screenOrientation="portrait" />
        <activity
            android:name=".UpdateEmployee"
            android:screenOrientation="portrait" />
        <activity
            android:name=".EmpSelection"
            android:screenOrientation="portrait" />
        <activity
            android:name=".StudentSelection"
            android:screenOrientation="portrait" />
        <activity
            android:name=".StudentProfile"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ExpenseReport"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ViewStudents"
            android:screenOrientation="portrait" />
        <activity
            android:name=".FeeManager"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ExpenseManager"
            android:screenOrientation="portrait" />
        <activity
            android:name=".StudentRegisteration"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>